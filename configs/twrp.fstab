/boot              emmc      /dev/block/mmcblk0p24	
/recovery          emmc      /dev/block/mmcblk0p25
/cache             ext4      /dev/block/mmcblk0p28
/system            ext4      /dev/block/mmcblk0p27
/data              ext4      /dev/block/mmcblk0p29
/external_sd       vfat      /dev/block/mmcblk1p1       /dev/block/mmcblk1  flags=display="External SDCard";storage;wipeingui;removable
/usb-otg           vfat      /dev/block/sda1            /dev/block/sda   flags=display="USB-OTG";storage;wipeingui;removable
